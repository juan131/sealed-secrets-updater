{
  "secrets": [
    {
      "name": "one-secret",
      "namespace": "default",
      "metadata": {
        "annotations": {
          "a-custom-annotation": "a-custom-annotation-value"
        }
      },
      "input": {
        "type": "file",
        "config": {
          "path": "examples/file/secrets/one-secret.json"
        }
      },
      "output": {
        "type": "file",
        "config": {
          "path": "examples/file/manifests/one-sealed-secret.json"
        }
      }
    },
    {
      "name": "another-secret",
      "namespace": "default",
      "metadata": {
        "labels": {
          "a-custom-label": "a-custom-label-value"
        }
      },
      "input": {
        "type": "file",
        "config": {
          "path": "examples/file/secrets/another-secret.json"
        }
      },
      "output": {
        "type": "file",
        "config": {
          "path": "examples/file/manifests/another-sealed-secret.json"
        }
      }
    }
  ]
}
